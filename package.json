    "maintenance:list": "ts-node scripts/maintenance.ts --list",
    "maintenance:task": "ts-node scripts/maintenance.ts --task",
    "export:michael": "ts-node --project tsconfig.scripts.json scripts/export-michael-usage.ts",
    "generate:michael-table": "ts-node --project tsconfig.scripts.json scripts/generate-michael-analytics-table.ts"
  },
